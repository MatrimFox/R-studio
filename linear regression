df2 <- read.xlsx("C:\\Users\\Matrim\\Desktop\\Универ 2.0\\Диплом\\Data_students.xlsx")
head(df)
tail(df)
str(df)
summary(df)
#df$Score <- as.numeric(df$Score)
#class(df$Score)

set.seed(101) 
sample <- sample.split(df2$Age, SplitRatio = 0.70) # SplitRatio = percent of sample==TRUE
train = subset(df2, sample == TRUE)
test = subset(df2, sample == FALSE)

model <- lm(Join ~ .,train)
summary(model)

predictions <- predict(model, newdata = test)

rmse <- sqrt(mean((test$Join - predictions)^2))
print(paste('RMSE:', rmse))

df2 <- df[,-1]
head(df2)
num.cols <- sapply(df2, is.numeric)
cor.data <- cor(df2[,num.cols])
cor.data

vif_values <- vif(model)
print(vif_values)
